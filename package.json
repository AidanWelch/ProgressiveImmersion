{
	"name": "progressiveimmersion",
	"version": "0.5.0",
	"description": "A browser extension for progressive immersion in a language.",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/AidanWelch/ProgressiveImmersion.git"
	},
	"keywords": [
		"language",
		"learning",
		"browser",
		"extension",
		"polyfill"
	],
	"scripts": {
		"pack": "npx webpack --mode=development",
		"serve-pack": "webpack serve --mode=development",
		"serve-ext": "npx web-ext run",
		"build": "npx webpack --mode=production && npx web-ext build"
	},
	"author": "Aidan Welch",
	"license": "Apache-2.0",
	"homepage": "https://github.com/AidanWelch/ProgressiveImmersion",
	"devDependencies": {
		"copy-webpack-plugin": "^10.2.4",
		"google-translate-api-x": "^10.4.1",
		"web-ext": "^6.8.0",
		"webextension-polyfill": "^0.7.0",
		"webpack": "^5.72.0",
		"webpack-cli": "^4.9.2",
		"webpack-dev-server": "^4.8.1"
	}
}
